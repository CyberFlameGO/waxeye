#!/bin/bash
source "${BASH_SOURCE%/*}/env.sh"

: ${NODE_BIN:="$(which nodejs || which node || log_error Please install nodejs && false)"}

rm -rf tmp/js/test_parser.js
mkdir -p tmp/js

run_waxeye -g javascript tmp/js/ -p test ./test/fixtures/env1.waxeye > /dev/null

NODE_PATH=src/javascript/ "$NODE_BIN" test/javascript/test.js